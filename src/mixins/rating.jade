mixin rating(name,label)
  .form-group
    .col-xs-10.control-label(id="rating-"+name)=label
    .col-xs-2
      rating(aria-labelledby="rating-"+name data-ng-model="akzeptanz.summary."+name data-ng-change="akzeptanz.rate('"+name+"')" aria-valuetext="{{akzeptanz.labels[akzeptanz.summary."+name+"]}}")