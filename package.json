{
  "name": "wbt-framework",
  "version": "2.3.3",
  "description": "Ein WBT-Framework",
  "main": "index.js",
  "scripts": {
    "start": "node --es_staging index.js",
    "test_client": "karma start --single-run",
    "test_server": "mocha tests/server/",
    "test": "npm run test_client && npm run test_server",
    "lint": "eslint api",
    "prebuild": "rm -f dist/*",
    "build:js": "NODE_ENV=production webpack -p",
    "build:compress": "zopfli dist/*.js",
    "build": "npm run build:js && npm run build:compress",
    "preversion": "npm run lint && npm test && npm run build && git add ./dist",
    "version": "npm run docs && git add ./docs",
    "postversion": "git push && git push --tags",
    "docs": "jsdoc -c jsdoc.json"
  },
  "author": "Tamara Cook <tamara.cook@icloud.com>",
  "license": "MIT",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/tamaracha/wbt-framework.git"
  },
  "bin": {
    "wbt-setup": "./bin/wbt-setup.js",
    "update-db": "./bin/update-db.js"
  },
  "apps": [
    {
      "script": "index.js",
      "node_args": "--es-staging",
      "env": {
        "port": 3000,
        "NODE_ENV": "production"
      },
      "post_update": [
        "echo updating deps",
        "npm i --production",
        "npm prune --production"
      ]
    }
  ],
  "devDependencies": {
    "angular": "^1.5.0",
    "angular-animate": "^1.5.",
    "angular-aria": "^1.5.0",
    "angular-file-upload": "^2.2.0",
    "angular-formly": "^7.5.0",
    "angular-formly-templates-bootstrap": "^6.2.0",
    "angular-messages": "^1.5.0",
    "angular-mocks": "^1.5.0",
    "angular-permission": "^1.1.1",
    "angular-sanitize": "^1.5.0",
    "angular-ui-bootstrap": "^1.1.1",
    "angular-ui-router": "^0.2.17",
    "angular-ui-router.statehelper": "^1.3.1",
    "api-check": "^7.5.5",
    "babel-core": "^6.4.5",
    "babel-loader": "^6.2.1",
    "babel-preset-es2015": "^6.3.13",
    "bootstrap": "^3.3.6",
    "css-loader": "^0.23.1",
    "eslint": "^1.10.3",
    "eslint-loader": "^1.2.1",
    "ink-docstrap": "^1.1.1",
    "isparta-loader": "^2.0.0",
    "jade": "^1.11.0",
    "jasmine-core": "^2.4.1",
    "jquery": "^2.2.0",
    "jsdoc": "^3.4.0",
    "json-loader": "^0.5.4",
    "karma": "^0.13.19",
    "karma-coverage": "^0.5.3",
    "karma-jasmine": "^0.3.6",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-webpack": "^1.7.0",
    "mocha": "^2.4.5",
    "ng-annotate-loader": "^0.1.0",
    "ngstorage": "^0.3.10",
    "ngtemplate-loader": "^1.3.0",
    "node-zopfli": "^1.4.0",
    "objectid-browser": "0.0.1",
    "oclazyload": "^1.0.9",
    "phantomjs-prebuilt": "^2.1.3",
    "script-loader": "^0.6.1",
    "source-map-loader": "^0.1.5",
    "style-loader": "^0.13.0",
    "swagger-js-codegen": "^1.1.8",
    "swagger-loader": "github:tamaracha/swagger-loader",
    "swagger-parser": "^3.4.0",
    "template-html-loader": "0.0.3",
    "wbt-text": "github:tamaracha/wbt-text",
    "webpack": "^1.12.12",
    "yaml-loader": "^0.1.0"
  },
  "dependencies": {
    "basic-auth": "^1.0.3",
    "bcrypt-nodejs": "0.0.3",
    "bluebird": "^3.2.0",
    "config": "^1.19.0",
    "debug": "^2.2.0",
    "dot": "^1.0.3",
    "fast-json-patch": "^0.5.6",
    "gridfs": "^1.0.0",
    "js-yaml": "^3.5.2",
    "jsonwebtoken": "^5.5.4",
    "kerberos": "0.0.18",
    "koa": "^1.1.2",
    "koa-body": "^1.4.0",
    "koa-dot": "^0.1.5",
    "koa-jwt": "^1.1.2",
    "koa-mount": "^1.3.0",
    "koa-onerror": "^1.2.1",
    "koa-qs": "^2.0.0",
    "koa-roles": "^1.0.2",
    "koa-router": "^5.3.0",
    "koa-send": "^3.1.0",
    "koa-static": "^2.0.0",
    "koa-unless": "0.0.1",
    "lodash": "^4.1.0",
    "mongoose": "^4.2.10",
    "mongoose-json-patch": "^1.1.1",
    "mongoose-validator": "^1.2.4",
    "yargs": "^3.32.0"
  }
}
